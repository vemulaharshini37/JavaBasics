import java.util.Scanner;

public class agechecker {
    public static void main(String[] args) {
        Scanner sc= new Scanner(System.in);
        System.out.println("please provide ur age:");
        int a = sc.nextInt();
        if(a>=18 && a<=59)
        {
            System.out.println("its legal to vote");
            System.out.println("You are younger than the seni" +
                    "or citizen age.");

        }
            if(a>=60)
            {
                System.out.println("its legal to vote");
                System.out.println("ur having citizen ship benefits");
            }
                if(a<18)
                {
                    System.out.println("ur younger peep");
                }
            }
        }

